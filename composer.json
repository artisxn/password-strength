{
    "name": "codicastudio/password-strength",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/password-strength",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.8",
        "illuminate/support": "^7.0",
        "illuminate/translation": "^7.0"
    },
    "require-dev": {
        "illuminate/validation": "^7.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\PasswordStrength\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\PasswordStrength\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\PasswordStrength\\PasswordStrengthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",

    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },

    "config": {
        "discard-changes": true
    }
}
